{
  "name": "@nylas/identity",
  "version": "2.0.0-canary.0",
  "description": "Nylas SDK for handling authentication and session management in browser and server",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/nylas/nylas.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Nylas Inc.",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "18.18.2",
    "@types/react": "18.0.35",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "prettier": "2.7.1",
    "typescript": "4.9.5",
    "vite": "^4.5.2"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "2.0.1",
    "buffer": "6.0.3",
    "js-sha256": "0.9.0",
    "jwt-decode": "3.1.2",
    "react": "18.2.0",
    "uuid": "8.3.2"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:common && npm run build:bundle",
    "build:esm": "tsc --build tsconfig.esm.json",
    "build:esm:watch": "tsc --build tsconfig.esm.json -w",
    "build:common": "tsc --build tsconfig.commonjs.json",
    "build:bundle": "vite build",
    "dev": "npx live-server dist/src --port=7000",
    "lint": "eslint . --ext .ts",
    "test": "echo 'No tests configured'",
    "format": "prettier --write ."
